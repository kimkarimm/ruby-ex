{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "wordpress-volume"
  },
  "spec": {
    "capacity": {
      "storage": "5Gi"
    },
    "accessModes": [
      "ReadWriteOnce"
    ],
    "nfs": {
      "path": "/OSEFront",
      "server": "master.lab.example.com"
    }
  }
}
